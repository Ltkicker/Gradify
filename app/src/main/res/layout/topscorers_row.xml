<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/background7"
        android:layout_width="423dp"
        android:layout_height="841dp"
        android:background="@drawable/img_background2"
        android:contextClickable="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/lower_rank4_bkg"
        tools:ignore="ContentDescription,ImageContrastCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="637dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="46dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_rank_bkg" />

    <ImageView
        android:id="@+id/lower_rank_num4_bkg"
        tools:ignore="ContentDescription,ImageContrastCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="652dp"
        android:layout_marginEnd="332dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_rank_num" />

    <TextView
        android:id="@+id/fullname"
        android:layout_width="143dp"
        android:layout_height="24dp"
        android:layout_marginStart="151dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="57dp"
        android:fontFamily="@font/poppins"
        android:hint="@string/student_fullname"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/lower_rank_dp"
        tools:ignore="ContentDescription,ImageContrastCheck"
        android:layout_width="33dp"
        android:layout_height="31dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="644dp"
        android:layout_marginEnd="282dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_default_profile" />

    <TextView
        android:id="@+id/lower_rank_num"
        tools:ignore="SmallSp,TextContrastCheck,VisualLintOverlap"
        android:layout_width="19dp"
        android:layout_height="14dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="653dp"
        android:layout_marginEnd="330dp"
        android:layout_marginBottom="64dp"
        android:hint="@string/sub_category_rank"
        android:textAlignment="center"
        android:textSize="9sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>