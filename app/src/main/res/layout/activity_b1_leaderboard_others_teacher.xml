        android:focusable="true"
        android:fontFamily="@font/poppins"
        android:hint="@string/others_category"
        android:textAlignment="center"
        android:textColor="#57AD55"
        android:textColorHint="#57AD55"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/others_category5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:clickable="true"
        android:focusable="true"
        android:fontFamily="@font/poppins"
        android:hint="@string/others_category"
        android:textAlignment="center"
        android:textColor="#57AD55"
        android:textColorHint="#57AD55"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/others_category4"
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/img_mainfloatingbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/quiz_text1"
        app:srcCompat="@drawable/img_Teachermainfloatingbox"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <TextView
        android:id="@+id/text_edit"
        android:layout_width="71dp"
        android:layout_height="20dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:clickable="true"
        android:fontFamily="@font/poppins"
        android:text="@string/main_floatedit"
        android:textAlignment="center"
        android:textColor="#80596F6B"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/img_editicon"
        app:layout_constraintTop_toBottomOf="@+id/quiz_text1"
        tools:ignore="KeyboardInaccessibleWidget,MissingConstraints,TextContrastCheck,TextSizeCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/img_editicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/icon_main_edit"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck"
