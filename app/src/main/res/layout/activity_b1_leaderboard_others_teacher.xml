        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:clickable="true"
        android:fontFamily="@font/poppins"
        android:text="@string/sub_category_date"
        android:textSize="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/category_bkg4"
        tools:ignore="HardcodedText,KeyboardInaccessibleWidget,SmallSp,SpeakableTextPresentCheck,TextContrastCheck,TouchTargetSizeCheck,VisualLintOverlap" />

    <ImageButton
