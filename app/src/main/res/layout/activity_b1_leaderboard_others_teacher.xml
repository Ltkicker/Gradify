        android:layout_height="31dp"
        android:layout_marginEnd="5dp"
        android:clickable="true"
        android:fontFamily="@font/poppins"
        android:hint="@string/category"
        android:textAlignment="center"
        android:textColor="#1AC171"
        android:textColorHint="#4E635E"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/category13"
        tools:ignore="HardcodedText,KeyboardInaccessibleWidget,VisualLintOverlap,TextContrastCheck,TextSizeCheck,TouchTargetSizeCheck,DuplicateSpeakableTextCheck"
