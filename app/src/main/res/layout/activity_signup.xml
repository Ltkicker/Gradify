<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/auth"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:background="@drawable/round_corner"
	android:backgroundTintMode="add"
	android:filterTouchesWhenObscured="true"
	android:textAlignment="center">


	<ImageView
		android:id="@+id/background2"
		android:layout_width="412dp"
		android:layout_height="730dp"
		android:contentDescription="@string/background_desc"
		android:scaleType="centerCrop"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.888"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0"
		app:srcCompat="@drawable/img_background2"
		tools:ignore="ImageContrastCheck" />

	<TextView
		android:id="@+id/textsignup"
		
		android:layout_width="201dp"
		android:layout_height="61dp"
		android:layout_marginBottom="18dp"
		android:layout_marginTop="656dp"
		android:inputType="textPassword"
		android:background="@drawable/shape_password"
		android:backgroundTintMode="src_over"
		android:clickable="true"
		android:ellipsize="middle"
		android:focusable="true"
		android:fontFamily="@font/poppins"
		android:gravity="center|center_horizontal|center_vertical"
		android:text="@string/sign_in"
		android:textAlignment="inherit"
		android:textAppearance="@style/TextAppearance.AppCompat.Medium"
		android:textColor="@color/sign_in"
		android:textDirection="inherit"
		android:textSize="20sp"

		android:textStyle="bold"
		android:visibility="visible"
		app:drawableBottomCompat="@drawable/round_corner"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_marginBaseline="10dp"
		tools:ignore="MissingConstraints"
		tools:visibility="visible" />

	<ImageView
		android:id="@+id/iconback"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="39dp"
		android:layout_marginTop="58dp"
		android:layout_marginEnd="326dp"
		android:layout_marginBottom="698dp"
		android:contentDescription="@string/background_desc"
		android:src="@drawable/iconback"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:ignore="ImageContrastCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_last_name"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:layout_marginTop="204dp"
		android:layout_marginEnd="26dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/last_name"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:ignore="MissingConstraints,TextContrastCheck,DuplicateSpeakableTextCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_first_name"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/first_name"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.518"
		app:layout_constraintStart_toStartOf="parent"
		tools:ignore="MissingConstraints,TextContrastCheck"
		tools:layout_editor_absoluteY="275dp" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_mddle_name"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/middle_name"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.464"
		tools:ignore="MissingConstraints,TextContrastCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_username"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/username"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.518"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.74"
		tools:ignore="MissingConstraints,TextContrastCheck,EditableContentDescCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_email"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/email"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.651"
		tools:ignore="MissingConstraints,TextContrastCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_password"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/password"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.825"
		tools:ignore="MissingConstraints,TextContrastCheck" />

	<com.google.android.material.textfield.TextInputEditText
		android:id="@+id/input_suffix"
		android:layout_width="339dp"
		android:layout_height="48dp"
		android:layout_marginTop="415dp"
		android:layout_marginEnd="27dp"
		android:background="@drawable/shape_signin"
		android:backgroundTint="#36FFEBEE"
		android:backgroundTintMode="src_over"
		android:drawableTint="#36FFFFFF"
		android:fontFamily="@font/poppins"
		android:hint="@string/suffix"
		android:textColor="#FFFFFF"
		android:textColorHint="#757575"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0"
		tools:ignore="MissingConstraints,TextContrastCheck" />

	<CheckBox
		android:id="@+id/cb_accept_terms"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:buttonTint="#FFFFFF"
		android:fontFamily="@font/poppins"
		android:text="@string/allterms"
		android:textColor="#FFFCFC"
		android:textSize="14sp"
		tools:ignore="MissingConstraints,TextContrastCheck"
		tools:layout_editor_absoluteX="28dp"
		tools:layout_editor_absoluteY="648dp" />

	<ImageView
		android:id="@+id/img_gradifybiglogo"
		android:layout_width="145dp"
		android:layout_height="134dp"
		android:layout_marginStart="93dp"
		android:layout_marginTop="32dp"
		android:layout_marginEnd="124dp"
		android:adjustViewBounds="false"
		android:backgroundTint="#59FFFDE7"
		android:scaleType="fitCenter"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/img_gradifybiglogo"
		tools:ignore="ContentDescription,ImageContrastCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>

