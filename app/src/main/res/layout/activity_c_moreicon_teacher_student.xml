<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bac"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">

    <View
        android:id="@+id/bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:background="@drawable/img_background2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="VisualLintBounds" />

    <Button
        android:id="@+id/img_home_button5"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/img_menu_home"
        android:backgroundTint="#CDFFEA"
        android:clickable="true"
        app:iconTint="#EDFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="KeyboardInaccessibleWidget,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/about_logo"
        android:layout_width="192dp"
        android:layout_height="32dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/about_cubix"
        tools:ignore="ContentDescription" />

    <View
        android:id="@+id/gren_bg"
        android:layout_width="417dp"
        android:layout_height="57dp"
        android:background="@drawable/version_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/about_logo"
        tools:ignore="VisualLintBounds" />

    <TextView
        android:id="@+id/version"
        android:layout_width="116dp"
        android:layout_height="37dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/text_version"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/v10"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_home_button5"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <TextView
        android:id="@+id/v10"
        android:layout_width="73dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/number_version"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/about_logo"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <View
        android:id="@+id/message_bg"
        android:layout_width="339dp"
        android:layout_height="133dp"
        android:layout_marginTop="54dp"
        android:background="@drawable/shape_signin"
        android:backgroundTint="#CDFFEA"
        android:backgroundTintMode="src_over"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg" />

    <TextView
        android:id="@+id/send_reports"
        android:layout_width="181dp"
        android:layout_height="37dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/poppins"
        android:text="@string/text_send_reports"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintEnd_toStartOf="@+id/switch2"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="44dp"
        android:minWidth="48dp"
        android:minHeight="48dp"
        android:shadowColor="#94FBAB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg"
        tools:ignore="UseSwitchCompatOrMaterialXml" />

    <TextView
        android:id="@+id/message1"
        android:layout_width="300dp"
        android:layout_height="73dp"
        android:layout_marginTop="84dp"
        android:fontFamily="@font/poppins"
        android:text="@string/text_mesage"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TextContrastCheck,TextSizeCheck,VisualLintOverlap" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="335dp"
        android:layout_height="1dp"
        android:layout_marginBottom="31dp"
        android:background="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/message_bg"
        tools:layout_editor_absoluteX="40dp">

    </LinearLayout>

    <TextView
        android:id="@+id/help_center"
        android:layout_width="89dp"
        android:layout_height="32dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="158dp"
        android:fontFamily="@font/poppins"
        android:text="@string/help_center"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintEnd_toStartOf="@+id/send2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <ImageView
        android:id="@+id/send2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="54dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gren_bg"
        app:srcCompat="@android:drawable/ic_menu_send"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <View
        android:id="@+id/terms"
        android:layout_width="239dp"
        android:layout_height="59dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/shape_signin"
        android:backgroundTint="#CDFFEA"
        android:backgroundTintMode="src_over"
        app:layout_constraintBottom_toTopOf="@+id/privacy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_terms"
        android:layout_width="165dp"
        android:layout_height="39dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/terms_service"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/privacy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,TextSizeCheck,VisualLintOverlap" />

    <View
        android:id="@+id/privacy"
        android:layout_width="239dp"
        android:layout_height="59dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/shape_signin"
        android:backgroundTint="#CDFFEA"
        android:backgroundTintMode="src_over"
        app:layout_constraintBottom_toTopOf="@+id/bg_cookies"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_privacy"
        android:layout_width="165dp"
        android:layout_height="39dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/privacy_policy"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/privacy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <View
        android:id="@+id/bg_cookies"
        android:layout_width="239dp"
        android:layout_height="59dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/shape_signin"
        android:backgroundTint="#CDFFEA"
        android:backgroundTintMode="src_over"
        app:layout_constraintBottom_toTopOf="@+id/bg_cookies2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_cookies_use"
        android:layout_width="165dp"
        android:layout_height="39dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/cookies_use"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/bg_cookies2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,TextSizeCheck" />

    <View
        android:id="@+id/bg_cookies2"
        android:layout_width="239dp"
        android:layout_height="59dp"
        android:layout_marginBottom="33dp"
        android:background="@drawable/shape_signin"
        android:backgroundTint="#CDFFEA"
        android:backgroundTintMode="src_over"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_cookies_use2"
        android:layout_width="165dp"
        android:layout_height="39dp"
        android:layout_marginBottom="45dp"
        android:fontFamily="@font/pragati_narrow"
        android:text="@string/legal_notices"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,TextSizeCheck" /><![CDATA[

    /LinearLayout>


]]>
</androidx.constraintlayout.widget.ConstraintLayout>
